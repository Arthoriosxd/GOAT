    -- Frame do usu√°rio (canto inferior esquerdo)
    local userFrame = Instance.new("Frame")
    userFrame.Name = "UserFrame"
    userFrame.Parent = mainFrame
    userFrame.Position = UDim2.new(0, 0, 1, -50)
    userFrame.Size = UDim2.new(0, 150, 0, 50)
