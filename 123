-- HUB KEY SYSTEM ROBLOX
-- Sistema de chave de acesso inspirado na interface mostrada
-- Criado por Claude AI

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local HttpService = game:GetService("HttpService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Chaves v√°lidas (voc√™ pode modificar estas)
local validKeys = {
    "SCRIPTHUB2024",
    "PREMIUM123",
    "VIPACCESS",
    "ARTHUB2024"
}

-- Fun√ß√£o para verificar se a key √© v√°lida
local function isValidKey(key)
    for _, validKey in pairs(validKeys) do
        if key == validKey then
            return true
        end
    end
    return false
end

-- Fun√ß√£o para criar a interface de key
